    "@ngx-env/builder": "^17.0.2-alpha.0",
    "@types/jasmine": "~5.1.0",
    "autoprefixer": "^10.4.16",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "postcss": "^8.4.31",
    "source-map-explorer": "^2.5.3",
    "tailwindcss": "^3.3.5",
    "typescript": "~5.2.2"
  }
